{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "functions": {
    "wsgi.py": {
      "includeFiles": "node_modules/**"
    }
  },
  "rewrites": [
    {
      "source": "/node_modules/(.*)",
      "destination": "/node_modules/$1"
    },
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/wsgi.py"
    }
  ]
}
