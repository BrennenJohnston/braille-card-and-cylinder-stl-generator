{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "env": {
    "PIP_ONLY_BINARY": ":all:"
  },

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ]
}
