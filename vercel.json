{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "buildCommand": "python -m pip install --upgrade pip && python -m pip install -r requirements.txt -t python",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "functions": {
    "wsgi.py": {
      "maxDuration": 60,
      "memory": 3008,
      "includeFiles": "python/**"
    }
  }
}
