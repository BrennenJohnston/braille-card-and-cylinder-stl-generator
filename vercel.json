{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "env": {
    "PIP_ONLY_BINARY": "shapely"
  },

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ]
}
