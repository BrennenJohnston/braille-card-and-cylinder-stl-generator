{
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "rewrites": [
    {
      "source": "/node_modules/liblouis-build/(.*)",
      "destination": "/static/liblouis/$1"
    },
    {
      "source": "/node_modules/liblouis/(.*)",
      "destination": "/static/liblouis/$1"
    }
  ],
  "headers": [
    {
      "source": "/static/liblouis/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
