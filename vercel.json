{
  "version": 2,
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.11"
      }
    }
  ],
  "installCommand": "pip3 install --upgrade pip && pip3 install -r requirements.txt -t python",
  "buildCommand": "true",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ]
}
